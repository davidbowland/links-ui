{
  "name": "links-ui",
  "version": "1.27.0",
  "description": "UI for links",
  "author": "David Bowland",
  "engines": {
    "node": "^18.0.0"
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidbowland/links-ui.git"
  },
  "bugs": {
    "url": "https://github.com/davidbowland/links-ui/issues"
  },
  "homepage": "https://github.com/davidbowland/links-ui#readme",
  "scripts": {
    "build": "npm run clean && gatsby build",
    "clean": "rm -rf .cache coverage public && npm ci --legacy-peer-deps && gatsby clean",
    "deploy": "DEVELOPMENT=true npm run build && ./scripts/deploy.sh",
    "lint": "prettier --write . && eslint --fix . --resolve-plugins-relative-to .",
    "prepare": "if [ \"$NODE_ENV\" != \"production\" ]; then husky install ; fi",
    "start": "gatsby develop",
    "serve": "npm run build && gatsby serve",
    "test": "jest --colors",
    "typecheck": "tsc --noEmit",
    "update": "ncu -u && npm i --legacy-peer-deps && npm audit fix --legacy-peer-deps && npm run test && npm dedupe --legacy-peer-deps"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "prettier --write --ignore-unknown",
      "eslint --fix --resolve-plugins-relative-to .",
      "jest --colors --findRelatedTests --coverageThreshold '{}' --passWithNoTests"
    ],
    "**/*": [
      "prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "@aws-amplify/ui-react": "^4.3.3",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fontsource/roboto": "^4.5.8",
    "@mdx-js/mdx": "^2.2.1",
    "@mdx-js/react": "^2.2.1",
    "@mui/icons-material": "^5.11.0",
    "@mui/lab": "^5.0.0-alpha.115",
    "@mui/material": "^5.11.4",
    "aws-amplify": "^5.0.10",
    "babel-plugin-styled-components": "^2.0.7",
    "crypto-browserify": "^3.12.0",
    "fast-json-patch": "^3.1.1",
    "gatsby": "^5.4.1",
    "gatsby-legacy-polyfills": "^3.4.0",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-image": "^3.4.0",
    "gatsby-plugin-mdx": "^5.4.0",
    "gatsby-plugin-react-helmet": "^6.4.0",
    "gatsby-plugin-sass": "^6.4.0",
    "gatsby-plugin-sharp": "^5.4.0",
    "gatsby-plugin-sitemap": "^6.4.0",
    "gatsby-plugin-styled-components": "^6.4.0",
    "gatsby-source-filesystem": "^5.4.0",
    "gatsby-transformer-sharp": "^5.4.0",
    "graphql": "^16.6.0",
    "qrcode.react": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "stream-browserify": "^3.0.0",
    "styled-components": "^5.3.6",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.18.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/react-helmet": "^6.1.6",
    "@types/styled-components": "^5.1.26",
    "babel-jest": "^29.3.1",
    "babel-preset-gatsby": "^3.4.0",
    "eslint-config-prettier": "^8.6.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "lint-staged": "^13.1.0",
    "msw": "^0.49.2",
    "prettier": "^2.8.3",
    "react-test-renderer": "^18.2.0",
    "ts-jest": "^29.0.5"
  }
}
