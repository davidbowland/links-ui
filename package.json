{
  "name": "links-ui",
  "version": "1.18.0",
  "description": "UI for links",
  "author": "David Bowland",
  "engines": {
    "node": "^18.0.0"
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidbowland/links-ui.git"
  },
  "bugs": {
    "url": "https://github.com/davidbowland/links-ui/issues"
  },
  "homepage": "https://github.com/davidbowland/links-ui#readme",
  "scripts": {
    "build": "npm run clean && gatsby build",
    "clean": "rm -rf .cache coverage public && npm ci --legacy-peer-deps && gatsby clean",
    "deploy": "DEVELOPMENT=true npm run build && ./scripts/deploy.sh",
    "lint": "prettier --write . && eslint --fix . --resolve-plugins-relative-to .",
    "prepare": "if [ \"$NODE_ENV\" != \"production\" ]; then husky install ; fi",
    "start": "gatsby develop",
    "serve": "npm run build && gatsby serve",
    "test": "jest --colors",
    "typecheck": "tsc --noEmit"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "prettier --write --ignore-unknown",
      "eslint --fix --resolve-plugins-relative-to .",
      "jest --colors --findRelatedTests --coverageThreshold '{}' --passWithNoTests"
    ],
    "**/*": [
      "prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "@aws-amplify/ui-react": "^3.2.1",
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@fontsource/roboto": "^4.5.7",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@mui/icons-material": "^5.8.4",
    "@mui/lab": "^5.0.0-alpha.93",
    "@mui/material": "^5.9.3",
    "aws-amplify": "^4.3.30",
    "axios": "^0.27.2",
    "babel-plugin-styled-components": "^2.0.7",
    "crypto-browserify": "^3.12.0",
    "fast-json-patch": "^3.1.1",
    "gatsby": "^4.20.0",
    "gatsby-legacy-polyfills": "^2.20.0",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-image": "^2.20.0",
    "gatsby-plugin-mdx": "^3.20.0",
    "gatsby-plugin-react-helmet": "^5.20.0",
    "gatsby-plugin-sass": "^5.20.0",
    "gatsby-plugin-sharp": "^4.20.0",
    "gatsby-plugin-sitemap": "^5.20.0",
    "gatsby-plugin-styled-components": "^5.20.0",
    "gatsby-source-filesystem": "^4.20.0",
    "gatsby-transformer-sharp": "^4.20.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "stream-browserify": "^3.0.0",
    "styled-components": "^5.3.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.18.6",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.3",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/react-helmet": "^6.1.5",
    "@types/styled-components": "^5.1.25",
    "babel-jest": "^28.1.3",
    "babel-preset-gatsby": "^2.20.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "lint-staged": "^13.0.3",
    "msw": "^0.44.2",
    "prettier": "^2.7.1",
    "react-test-renderer": "^18.2.0",
    "ts-jest": "^28.0.7"
  }
}
